{
	"compilerOptions": {
	  "target": "es6",
	  "module": "esnext",
	  "strict": true,
	  "jsx": "preserve",
	  "moduleResolution": "node",
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*.ts", "src/**/*.svelte"],
	"exclude": ["node_modules"]
  }
  